{
	"applicationId": "moe.curstantine.melody",
	"version": "0.1.0",
	"defaultMode": "window",
	"documentRoot": "/app/dist/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": [
		"app.*",
		"window.*",
		"filesystem.readDirectory"
	],
	"modes": {
		"window": {
			"title": "melody",
			"width": 800,
			"height": 500,
			"minWidth": 800,
			"minHeight": 500,
			"icon": "/app/dist/logo.svg",
			"enableInspector": true,
			"borderless": true,
			"maximize": false,
			"hidden": false,
			"resizable": true,
			"exitProcessOnClose": false
		}
	},
	"cli": {
		"binaryName": "melody",
		"resourcesPath": "/app/dist",
		"extensionsPath": "/extensions/",
		"binaryVersion": "4.10.0",
		"clientVersion": "3.8.2",
		"clientLibrary": "/app/public/neutralino.js",
		"frontendLibrary": {
			"devUrl": "http://localhost:5173"
		}
	}
}
